{
    "compilerOptions": {
      "jsx": "react",
      "module": "commonjs",
      "target": "es5",
      "noImplicitAny": false,
      "removeComments": true,
      "preserveConstEnums": true,
      "outDir": "dist",
      "sourceMap": true,
      "esModuleInterop": true,
    },
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "node_modules"
    ]
}